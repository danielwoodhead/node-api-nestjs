{
	"name": "node-api-nestjs",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"esbenp.prettier-vscode",
				"ms-mssql.mssql",
				"prisma.prisma"
			]
		}
	},
	"features": {
		"docker-from-docker": {
			"version": "latest",
			"moby": true,
			"dockerDashComposeVersion": "v2"
		}
	},
	"remoteUser": "node"
}